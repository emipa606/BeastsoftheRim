<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="EggFertBase">
    <defName>EggPenguinFertilized</defName>
    <label>penguin egg (fert.)</label>
    <description>Fertilized penguin egg. If all goes well, it should hatch into a baby penguin. It can be eaten raw, but it's much better cooked.</description>
    <graphicData>
      <color>(236,217,184)</color>
      <drawSize>1.1</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>11.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>5</hatcherDaystoHatch>
        <hatcherPawn>Penguin</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
